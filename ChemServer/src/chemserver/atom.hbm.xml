<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="persist.AtomModel" table="ATOM">
    <id column="ID" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="TYPE" name="type" type="string"/>
    <property column="X" name="x" type="integer"/>
    <property column="Y" name="y" type="integer"/>
    <property column="DOCUMENT_ID" name="documentId" type="integer"/>
  </class>
</hibernate-mapping>
