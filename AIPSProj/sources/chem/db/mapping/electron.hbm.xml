<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="chem.figures.persist.ElectronModel" table="ELECTRON">
  
      <id name="id" type="integer" column="ID">
          <generator class="increment"></generator>
      </id>
      
      <property name="atomX" column="ATOM_X" type="integer"></property>
      <property name="atomY" column="ATOM_Y" type="integer"></property>
      <property name="index" column="INDEX" type="integer"></property>
      
      <property name="angle" column="ANGLE" type="integer" ></property>
      
      <property name="documentId" column="DOCUMENT_ID" type="integer" ></property>
      
  </class>
  
</hibernate-mapping>
