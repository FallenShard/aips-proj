<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="chem.figures.persist.BondModel" table="BOND">
  
    <id name="id" type="integer" column="ID">
          <generator class="increment"></generator>
    </id>

    <property name="startAtomX" column="START_ATOM_X" type="integer" ></property>
    <property name="startAtomY" column="START_ATOM_Y" type="integer"></property>
    <property name="startElectronIndex" column="START_ELECTRON_INDEX" type="integer"></property>
    
    <property name="endAtomX" column="END_ATOM_X" type="integer" ></property>
    <property name="endAtomY" column="END_ATOM_Y" type="integer"></property>
    <property name="endElectronIndex" column="END_ELECTRON_INDEX" type="integer"></property>

    <property name="documentId" column="DOCUMENT_ID" type="integer"></property>
  </class>

</hibernate-mapping>
