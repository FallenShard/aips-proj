<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="chem.figures.persist.ChemicalBondModel" table="CHEMICALBOND">
      
      <id name="id" type="integer" column="ID">
          <generator class="increment"></generator>
      </id>
      
      
      <property name="startElectronId" column="START_ELECTRON_ID" type="integer"></property>
      <property name="endElectronId" column="END_ELECTRON_ID" type="integer"></property>
      <property name="documentId" column="DOCUMENT_ID" type="integer"></property>
      
      
  </class>
</hibernate-mapping>
